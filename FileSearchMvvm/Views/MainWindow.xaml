<Window x:Class="FileSearchMvvm.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileSearchMvvm.Views"
        xmlns:viewModels="clr-namespace:FileSearchMvvm.ViewModels"
        mc:Ignorable="d"
        Title="File Search" Height="600" Width="650">
    <Window.Resources>
        <local:SearchView x:Key="searchView"/>
    </Window.Resources>
    <Grid>
        <StaticResource ResourceKey="searchView"/>
    </Grid>
</Window>
